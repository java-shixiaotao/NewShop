<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yq.dao.ViewGiftDetailDao" >

  <select id="selectByGiftId" resultType="com.yq.entity.ViewGiftDetail" parameterType="java.lang.String" >
    select
    *
    from view_gift_detail
    where gift_id = #{giftId}
  </select>
</mapper>